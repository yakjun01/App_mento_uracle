{
  "name": "@morpheus/cli",
  "description": "Morpheus CLI Program",
  "version": "1.2.7",
  "type": "module",
  "main": "dist/main.js",
  "license": "SEE LICENSE",
  "bin": {
    "morpheus": "bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/embed"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w --sourcemap",
    "prepack": "yarn build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "test": "mocha test/**/*.test.ts -r ts-node/register"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "@eslint/js": "^9.20.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/chai": "^5.0.1",
    "@types/cross-spawn": "^6.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18",
    "@types/plist": "^3.0.5",
    "@types/superagent": "^8.1.9",
    "@types/tar-stream": "^3.1.3",
    "@types/yauzl": "^2.10.3",
    "@types/yazl": "^2.4.6",
    "chai": "^5.2.0",
    "commander": "^13.1.0",
    "cross-spawn": "^7.0.6",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "fast-xml-parser": "^5.0.8",
    "fs-extra": "^11.3.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "mocha": "^11.1.0",
    "plist": "^3.1.0",
    "prettier": "^3.5.0",
    "rimraf": "^5",
    "rollup": "^4.34.6",
    "semver": "^7.7.1",
    "superagent": "^10.1.1",
    "tar-stream": "^3.1.7",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yauzl": "^3.2.0",
    "yazl": "^3.3.1"
  }
}
